<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="360.0" prefWidth="590.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ResultController">
   <children>
      <ImageView fitHeight="401.0" fitWidth="601.0" layoutX="1.0" layoutY="-2.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@background.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="ResultTableView" layoutX="44.0" layoutY="41.0" prefHeight="308.0" prefWidth="266.0">
        <columns>
          <TableColumn fx:id="Results" prefWidth="59.0" text="Results" />
          <TableColumn fx:id="Date" prefWidth="71.0" text="Date" />
            <TableColumn fx:id="ExerciseId" prefWidth="135.0" text="Exersice name:" />
        </columns>
      </TableView>
      <Button layoutX="424.0" layoutY="306.0" mnemonicParsing="false" onAction="#backButton" styleClass="toggle-button" stylesheets="@Button.css" text="Back to menu">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
