<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #f3faff;" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.TaskWindowController">
